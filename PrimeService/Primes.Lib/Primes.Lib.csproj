<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
    <NoWarn>$(NoWarn);1591</NoWarn>
    <!-- NuGet Package Metadata -->
    <PackageId>Primes.Lib</PackageId>
    <Version>1.0.0</Version>
    <Authors>Angel Vargas Gutierrez</Authors>
    <Company>Universidad</Company>
    <Description>Biblioteca para verificar n√∫meros primos del 2 al 20</Description>
    <PackageTags>prime;numbers;math;utility</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/username/PrimeService</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

</Project> 